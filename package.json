    "body-parser": "^1.17.2",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "dropzone": "^5.4.0",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
